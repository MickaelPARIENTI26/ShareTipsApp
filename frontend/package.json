  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/detox": "^17.14.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "detox": "^20.47.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "^54.0.17",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
